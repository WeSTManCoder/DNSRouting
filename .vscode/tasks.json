{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Linux compile RAMIPS",
            "type": "shell",
            "command": "go build",
            "options": {
                "env": {
                    "GOOS": "linux",
                    "GOARCH": "mipsle",
                    "GOMIPS": "softfloat",
                    "GOFLAGS": "-ldflags=-s -w"
                }
            }
        }
    ]
}